{
	"name": "ROBLOX Studio",
	"description": "ROBLOX Studio in the browser",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": [],
	"host_permissions": [
		"https://*.roblox.com/*"
	],
	"action": {
		"default_popup": "index.html",
		"default_title": "ROBLOX Studio"
	},
	"icons": {
		"16": "unicorn.png",
		"32": "unicorn.png",
		"48": "unicorn.png",
		"128": "unicorn.png"
	},
	"content_security_policy": {
		"extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
	}
}